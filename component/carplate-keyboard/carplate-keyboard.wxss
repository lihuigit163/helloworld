/* carplatt-keyboard.wxss */

.ipt-container {
    display: flex;
    flex-flow: row;
    justify-content: space-around;
}

.ipt-container text {
    width: 66rpx;
    height: 88rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 8rpx;
    border: 1rpx solid rgba(216, 216, 216, 1);
    font-size: 32px;
    font-weight: 400;
    color: rgba(34, 34, 34, 1);
    line-height: 45px;
    text-align: center;
}

#ipt-newpower {
    font-size: 18rpx;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    line-height: 88rpx;

}
